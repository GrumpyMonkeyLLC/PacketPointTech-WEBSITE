{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{svg,png,jpg,gif}", "/css/*", "/js/*"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".svg": "image/svg+xml"
  },
  "globalHeaders": {
    "cache-control": "public, max-age=31536000, immutable"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
